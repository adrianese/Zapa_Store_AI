# Environment Variables for Zstore Contracts
# Copy this file to .env and fill in your values

# Private key for deploying contracts (without 0x prefix)
# WARNING: Never commit your real private key!
PRIVATE_KEY=your_private_key_here

# RPC URLs
SEPOLIA_RPC_URL=https://rpc.sepolia.org
MAINNET_RPC_URL=https://eth.llamarpc.com

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Platform wallet address (receives fees)
PLATFORM_WALLET=0xYourPlatformWalletAddress

# Platform fee percentage (in basis points, 250 = 2.5%)
PLATFORM_FEE_BPS=250
